{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
        },        
    "rules": {
        "indent": ["warn", 4, {
            "SwitchCase": 1
        }],
        "brace-style": "warn",
        "prefer-const": "warn",
        "comma-spacing": "error",
        "semi": "error",
        "quotes": ["error", "single"],
        "no-console": "off",
        "eqeqeq": "warn",
        "camelcase": "error",
        "no-var": "error",
        "prefer-template": "error",
        "spaced-comment": "error",
        "multiline-comment-style": ["error", "starred-block"]
    }
}